{
   "name": "django-ecommerce",
  "version": "1.0.0",
  "description": "Django ecommerce project with Tailwind CSS",
  "scripts": {
    "build-css": "tailwindcss -i ./src/assets/input.css -o ./src/static/css/output.css --watch",
    "build-css-prod": "tailwindcss -i ./src/assets/input.css -o ./src/static/css/output.css --minify"
  },
  "dependencies": {
    "tailwindcss": "^3.3.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10"
  }
}